{
  "name": "guess-to-survive",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "test:unit": "jest",
    "build": "tsc -b && vite build",
    "start": "vite preview",
    "fix": "npx ultracite fix",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test --project=chromium",
    "test:e2e:all": "playwright test",
    "test:e2e:coverage": "rm -rf .nyc_output coverage/e2e && mkdir -p .nyc_output && E2E_COVERAGE=true playwright test --project=chromium && nyc report --report-dir coverage/e2e --reporter=text-summary --reporter=html --reporter=lcov && nyc check-coverage --branches 85 --functions 85 --lines 85 --statements 85",
    "check": "npx ultracite check",
    "test:coverage": "jest --coverage",
    "local": "dotenv -e .env -- next dev",
    "env:pull": "npx dotenv-vault@latest pull",
    "env:push": "npx dotenv-vault@latest push",
    "dev": "vite",
    "commitlint": "npx --no -- commitlint --edit",
    "prod": "dotenv -e .env.production -- next dev"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.159.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.345.2",
    "radix-ui": "^1.4.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@commitlint/types": "^20.4.0",
    "@playwright/test": "^1.58.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.3",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router-devtools": "^1.159.5",
    "@tanstack/router-plugin": "^1.159.5",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "nyc": "^17.1.0",
    "semantic-release": "^25.0.3",
    "semantic-release-ado": "^1.4.0",
    "shadcn": "^3.8.4",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "ultracite": "^7.1.5",
    "vite": "^7.3.1",
    "vite-plugin-istanbul": "^7.2.1"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "**/*.d.ts",
      "src/routeTree.gen.ts",
      "src/types/**"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/components/protected-route.tsx",
      "src/routes/index.tsx",
      "src/routes/spending-history.tsx"
    ],
    "temp-dir": ".nyc_output"
  },
  "packageManager": "pnpm@10.28.2"
}
